{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "{Timestamp:dd.MM.yy HH:mm:ss.fff} [{Level:u3}] {Message:lj} {NewLine}{Exception}"
      }
    },
    "Properties": {
      "Application": "PublicApi"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "None"
    }
  },
  "AllowedHosts": "*",
  "GrpcPort": 8088,
  "InternalApi": {
    "CacheLifeTimeHours": 2,
    "Uri": "https://api.currencyapi.com/v3/",
    "BaseCurrency": "USD"
  },
  "ConnectionStrings": {
    "InternalApiDbContext": "Host=localhost;Port=5432;Database=internalapidb;Username=postgres;Password=Password1"
  }
}
