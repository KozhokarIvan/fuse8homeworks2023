{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "{Timestamp:dd.MM.yy HH:mm:ss.fff} [{Level:u3}] {Message:lj} {NewLine}{Exception}"
      }
    },
    "Properties": {
      "Application": "PublicApi"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "None"
    }
  },
  "AllowedHosts": "*",
  "GrpcUri": "http://localhost:8088",
  "PublicApi": {
    "Uri": "https://api.currencyapi.com/v3/",
    "CurrencySettingName": "defaultCurrency",
    "DecimalPlacesSettingName": "currencyRoundCount"
  },
  "ConnectionStrings": {
    "PublicApiDbContext": "Host=localhost;Port=5432;Database=publicapidb;Username=postgres;Password=Password1"
  }
}